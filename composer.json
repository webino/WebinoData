{
    "name":        "webino/webino-data",
    "description": "Modular Data service over Zend Db",
    "type":        "library",
    "license":     "BSD-3-Clause",
    "keywords":    ["data", "service", "webino", "zf2"],
    "homepage":    "https://github.com/webino/WebinoData",
    "authors": [
        {
            "name":     "Webino, s. r. o.",
            "email":    "info@webino.sk",
            "homepage": "http://webino.sk",
            "role":     "Developer"
        },
        {
            "name":     "Peter Bačinský",
            "email":    "peter@bacinsky.sk",
            "homepage": "http://bacinsky.sk",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*",
        "zendframework/zend-cache": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-paginator": "2.*",
        "zendframework/zend-inputfilter": "2.*",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "webino/webino-i18n-sanitize-lib": "dev-develop",
        "webino/zend-db": "~2.7.1"
    },
    "suggest": {
        "zendframework/zend-mvc": "To use as a Zend Framework module",
        "webino/webino-debug": "Debugger support"
    },
    "autoload": {
        "psr-0": {"WebinoData": "src/"},
        "psr-4": {"Webino\\": "src/"}
    },
    "extra": {
        "webino-devkit": {
            "context": ["module", "github", "zend"],
            "ZendSkeletonApplication": "release-2.5.0"
        }
    }
}
