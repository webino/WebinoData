{
    "name":        "webino/webino-data",
    "description": "Modular data service for Zend Framework 2",
    "type":        "library",
    "license":     "BSD-3-Clause",
    "keywords":    ["data", "service", "webino", "zf2"],
    "homepage":    "https://github.com/webino/WebinoData",
    "authors": [
        {
            "name":     "Webino, s. r. o.",
            "email":    "info@webino.sk",
            "homepage": "http://webino.sk",
            "role":     "Developer"
        },
        {
            "name":     "Peter Bačinský",
            "email":    "peter@bacinsky.sk",
            "homepage": "http://bacinsky.sk",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "zendframework/zend-db": "2.*",
        "zendframework/zend-cache": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-paginator": "2.*",
        "zendframework/zend-inputfilter": "2.*",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-servicemanager": "2.*",
        "webino/webino-i18n-sanitize-lib": "dev-develop"
    },
    "suggest": {
        "zendframework/zend-mvc": "To use as a Zend Framework 2 module",
        "webino/webino-debug": "Debugger support"
    },
    "require-dev": {
        "webino/webino-dev": "dev-develop",
        "webino/webino-debug": "dev-develop",
        "zendframework/zend-mvc": "2.6.*",
        "zendframework/zend-view": "2.*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-version": "2.*",
        "zendframework/zend-console": "2.*",
        "zendframework/zend-log": "2.*",
        "zendframework/zend-math": "2.*",
        "zendframework/zend-form": "2.6.*",
        "zendframework/zend-i18n-resources": "2.*"
    },
    "autoload": {"psr-0": {"WebinoData": "src/"}},
    "extra": {
        "webino-devkit": {
            "context": ["module", "github", "zend"],
            "ZendSkeletonApplication": "release-2.5.0"
        }
    }
}
